                                <input type="text" name="query" placeholder="Öğrenci adı, konumu, yetenekleri, vs. ara.." class="form-control" />
                            </form>
                        </div>
                        
                    </div>
                </div>
               


                <!-- Resume item -->
                <div class="col-xs-12">
                    <div class="item-block">
