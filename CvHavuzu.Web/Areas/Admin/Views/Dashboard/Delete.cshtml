@model CvHavuzu.Web.Models.ApplicationUser

@{
    ViewData["Title"] = "Kullanıcı Sil";
}

<h2>Kullanıcı Sil</h2>

<h3>Silmek istediğinize emin misiniz?</h3>
<div><form asp-action="Delete">
    <hr />
    <dl class="dl-horizontal">
        <input type="hidden" asp-for="Id" />
        <dt>
           Kullanıcı Adı:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt>
           Normalize Kullanıcı Adı:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NormalizedUserName)
        </dd>
        <dt>
           E-Posta:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt>
            Normalize E-Posta:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NormalizedEmail)
        </dd>
        <dt>
            E-Posta Doğrulandı mı?
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EmailConfirmed)
        </dd>
        <dt>
           Telefon Numarası:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
        <dt>
           Telefon Numarası Doğrulandı mı?
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PhoneNumberConfirmed)
        </dd>
        <dt>
           İki Aşamalı Doğrulama:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TwoFactorEnabled)
        </dd>
        <dt>
          Kilitleme Sonu:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LockoutEnd)
        </dd>
        <dt>
            Kilitleme Aktif mi?
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LockoutEnabled)
        </dd>
        <dt>
           Erişim Başarısızlığı Sayısı:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AccessFailedCount)
        </dd>
    </dl>


    <div class="form-actions no-color">
        <input type="submit" value="Sil" class="btn btn-danger" /> 
        <a asp-action="Users">Listeye geri dön</a>
    </div>
</form>
</div>
